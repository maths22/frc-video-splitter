{
  "license": "MIT",
  "name": "frc-video-splitter",
  "description": "Split FRC match videos into individual matches",
  "version": "4.0.0-alpha1",
  "author": "Ty Tremblay <ty.tremblay@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tytremblay/frc-video-splitter"
  },
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder",
    "release": "electron-builder --publish always",
    "updateVersion": "semantic-release"
  },
  "release": {
    "branches": [
      "main",
      "next",
      "alpha"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "semantic-release-export-data",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffmpeg/ffmpeg": "^0.12.7",
    "@ffmpeg/util": "^0.12.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.23",
    "@types/luxon": "^3.3.3",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "luxon": "^3.4.3",
    "react-player": "^2.13.0",
    "semantic-release": "^22.0.7",
    "semantic-release-export-data": "^1.0.1",
    "tailwind-merge": "^2.0.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "autoprefixer": "^10.4.16",
    "electron": "^26.2.2",
    "electron-builder": "^24.6.4",
    "next": "^12.3.4",
    "nextron": "^8.12.0",
    "postcss": "^8.4.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.0.2"
}
